{
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^react",
		"^react-dom",
		"^react(.*)$",
		"^routes(.*)$",
		"^services(.*)$",
		"^queries(.*)$",
		"^pages(.*)$",
		"^components(.*)$",
		"^utils(.*)$",
		"^\\./",
		"^\\../"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
